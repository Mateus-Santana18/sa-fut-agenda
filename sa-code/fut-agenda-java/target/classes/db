create table usuario (
	"id" serial primary key,
	"nome" varchar(40) not null,
	"email" text not null,
	"telefone" varchar(20) not null,
	"senha" varchar(30) not null
);

create table organizador (
	"id" serial primary key,
	"id_horario" integer references horario(id),
	"id_usuario" integer references usuario(id)
);

create table horario (
	"id" serial primary key,
	"dia_e_hora" timestamp  not null,
	"disponibilidade" varchar not null,
);

create table quadra (
	"id" serial primary key,
	"tipo" varchar(20) not null,
	"id_horario" integer references horario(id)
);

create table estabelecimento (
	"id" serial primary key,
	"endereco" varchar(40) not null,
	"cnpj" varchar(16) not null,
	"horario_abertura" timestamp not null,
	"horario_encerramento" timestamp not null,
	"id_usuario" integer references usuario(id),
	"id_quadra" integer references quadra(id)
);

create table agendamento (

)

